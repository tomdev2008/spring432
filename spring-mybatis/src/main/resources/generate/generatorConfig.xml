<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration>

	<!-- 
	    http://www.mybatis.org/generator/configreference/xmlconfig.html
		<properties resource="init.properties"/>
	-->

	<!-- 指定数据连接驱动jar地址 -->
    <classPathEntry  location="D:/gtp/maven/repo/mysql/mysql-connector-java/5.1.34/mysql-connector-java-5.1.34.jar" />
    
    <!-- 一个数据库一个context -->
	<context id="gtp_test" targetRuntime="MyBatis3" >		
		
		<commentGenerator>
			<property name="suppressAllComments" value="true" />
		</commentGenerator>
		
		<jdbcConnection driverClass="com.mysql.jdbc.Driver"
						connectionURL="jdbc:mysql://192.168.1.157:3306/mapsearch?characterEncoding=utf8&amp;autoReconnect=true"
						userId="b2c"
						password="b2c123" />
		
		<!-- 生成实体类地址 -->
		<javaModelGenerator targetPackage="com.mvw.generator"  
			targetProject="spring-mybatis" />
		
		<!-- 生成mapxml文件 -->	
		<sqlMapGenerator targetPackage="sqlmap"  
			targetProject="spring-mybatis"/>
			
		<!-- 生成mapxml对应接口dao -->
		<javaClientGenerator targetPackage="com.mvw.generator" 
			targetProject="spring-mybatis"
			type="XMLMAPPER" />

		<!-- 具体的表 -->
		<table tableName="car_trace" domainObjectName="CarTraceDO" 
				enableCountByExample="false" 
				enableUpdateByExample="false" 
				enableDeleteByExample="false"
			    enableSelectByExample="false" 
			    selectByExampleQueryId="false">
		</table>
		
		<table tableName="car_loc" domainObjectName="CarLocDO" 
				enableCountByExample="false" 
				enableUpdateByExample="false" 
				enableDeleteByExample="false"
			    enableSelectByExample="false" 
			    selectByExampleQueryId="false">
		</table>
	</context>
</generatorConfiguration>